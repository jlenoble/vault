{
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"editor.insertSpaces": false,
	"editor.detectIndentation": false,

	"files.associations": {
		"**/package.json": "json",
		"**/*.json": "jsonc",
		"**/*.css": "tailwindcss"
	},
	"files.trimTrailingWhitespace": true
}
